{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|html|webp|jpg|jpeg|png|gif|ico))",
      "dest": "public/$1"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ]
}
